{
    "name": "artisanry/doorkeeper",
    "description": "Compare the amount of relations with specified rules.",
    "keywords": ["laravel", "framework", "Laravel-DoorKeeper", "Laravel DoorKeeper"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.6",
        "illuminate/database": "^5.6"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Doorkeeper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Doorkeeper\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Doorkeeper\\DoorkeeperServiceProvider"
            ],
            "aliases": {
                "Doorkeeper": "Artisanry\\Doorkeeper\\Facades\\Doorkeeper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
